{
  "train_batch_size": 8,
  "gradient_accumulation_steps": 1,
  "steps_per_print": 1,
  "optimizer": {
    "type": "AdamW",
    "params": {
      "lr": 2e-5,
      "betas": [
        0.8,
        0.999
      ],
      "eps": 1e-8,
      "weight_decay": 3e-7
    }
  },
  "tensorboard": {
    "enabled": true,
    "output_path": "/tmp/output/log/ds_logs/",
    "job_name": "LPB_tensor"
  },
  "csv_monitor": {
    "enabled": true,
    "output_path": "/tmp/output/log/ds_logs/",
    "job_name": "LPB_csv"
  }
}